SinglyLinkedListNode* reverse(SinglyLinkedListNode* llist) {
    struct SinglyLinkedListNode *p = llist;
    int i = 0;
    int aArr[20];
    while(p!=NULL) {
        aArr[i] = p->data;
        p = p->next;
        i++;
    }
    p=llist; i--;
    while(p!=NULL) {
        p->data = aArr[i--];
        p = p->next;
    }
    return llist;
}
